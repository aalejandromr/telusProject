<div class="form-actions">
    <a class="btn medium" href="{{ "{{ path('" ~ route_name_prefix ~ "') }}" }}">Regresar a la lista</a>
    {% if ('edit' in actions) and (not hide_edit) %}
        <a class="btn medium btn-primary" href="{{ "{{ path('" ~ route_name_prefix ~ "_edit', { 'id': entity.id }) }}" }}">Editar</a>
    {% endif %}
</div>
<div class="form-actions">
    {% if ('delete' in actions) and (not hide_delete) %}
        {{ '{{ form(delete_form) }}' }}
    {% endif %}
</div>
